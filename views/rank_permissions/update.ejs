<h1>Actualizar Permiso de Rango</h1>
<form action="/api/rank_permissions/<%= rankPermission.rankId %>/<%= rankPermission.permissionId %>" method="POST">
    <input type="hidden" name="_method" value="PUT">
    <label for="rankId">ID del Rango:</label>
    <input type="number" id="rankId" name="rankId" value="<%= rankPermission.rankId %>" required>
    <br>
    <label for="permissionId">ID del Permiso:</label>
    <input type="number" id="permissionId" name="permissionId" value="<%= rankPermission.permissionId %>" required>
    <br>
    <button type="submit">Actualizar</button>
</form>
