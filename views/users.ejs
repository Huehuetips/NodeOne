<h2>Users Page</h2>
<table border="1" id="usersTable">
    <thead>
        <tr>
            <th>ID</th>
            <th>Username</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        <% users.forEach(user => { %>
            <tr>
                <td><%= user.id %></td>
                <td><%= user.username %></td>
                <td><%= user.password %></td>
            </tr>
        <% }) %>
    </tbody>
</table>
