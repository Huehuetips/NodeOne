    <input type="text" id="nameUser" name="nameUser" class="form-input">
    <br>
    <label for="userUser" class="form-label">Usuario:</label>
    <input type="text" id="userUser" name="userUser" class="form-input">
    <br>
    <label for="emailUser" class="form-label">Correo Electr√≥nico:</label>
    <input type="email" id="emailUser" name="emailUser" class="form-input">
    <br>
